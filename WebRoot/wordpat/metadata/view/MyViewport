                                    "\r",
                                    "function combine(a,b){\r",
                                    "    var aa = a.split(\"序#\")[1].split(\"&\");\r",
                                    "    var wc = a.split(\"#\");\r",
                                    "    var bc = wc[0];\r",
                                    "    var key1 =[];\r",
                                    "    for(var j = 0; j < aa.length; j++) {\r",
                                    "        key1.push(aa[j]);\r",
                                    "    }\r",
                                    "    var key2 =[];\r",
                                    "    var bb = b.split(\"&\");\r",
                                    "     for(var k = 0; k < bb.length; k++) {\r",
                                    "        key2.push(bb[k]);\r",
                                    "    }\r",
                                    "\r",
                                    "    var returnStr =\"\";\r",
                                    "    for(var i=0;i<key2.length;i++){\r",
                                    "        var flag = false;\r",
                                    "        var keystr2 = key2[i].split(\"=\")[0];\r",
                                    "        var value2 = key2[i].split(\"=\")[1].split(\"<!\")[1].split(\">\")[0];\r",
                                    "        for(var n =0;n<key1.length;n++){\r",
                                    "            var keystr1 = key1[n].split(\"=\")[0];\r",
                                    "            if(keystr2==keystr1){\r",
                                    "                flag=true;\r",
                                    "                continue;\r",
                                    "            }\r",
                                    "        }\r",
                                    "        if(!flag){\r",
                                    "            if(bc.indexOf(value2)==-1){\r",
                                    "                bc = bc+\"*[\"+value2+\"]\";\r",
                                    "            }\r",
                                    "             returnStr = returnStr +\"&\"+key2[i];\r",
                                    "\r",
                                    "        }\r",
                                    "    }\r",
                                    "\r",
                                    "    returnStr = bc+\"#\"+wc[1]+\"#\"+wc[2]+returnStr;\r",
                                    "    return returnStr;\r",
                                    "\r",
                                    "\r",
                                    "}\r",
                                    "\r",
                                    "String.prototype.startWith=function(str){\r",
                                    "  var reg=new RegExp(\"^\"+str);\r",
                                    "  return reg.test(this);\r",
                                    "};"
