                                    "                    param[\"next_abs_names\"]=next_abs_names;\r",
                                    "\r",
                                    "                    //调用Ajax方法，同步请求获取数据\r",
                                    "                    Ext.Ajax.request({\r",
                                    "                        url: '../wordpat.action',\r",
                                    "                        method: 'post',\r",
                                    "                        params: {\r",
                                    "                            action:\"simpleInsert\",\r",
                                    "                            param: Ext.JSON.encode(param)\r",
                                    "                        },\r",
                                    "                        async: false, //同步请求数据\r",
                                    "                        //获取数据成功\r",
                                    "                        success: function (result, request) {\r",
                                    "                            var retobj = Ext.JSON.decode(result.responseText);\r",
                                    "                            Ext.MessageBox.alert('保存结果', retobj.checkInfo);\r",
                                    "                            if(editstatus==\"1111\"){\r",
                                    "                                if(retobj.checkInfo=='更新成功！'){\r",
                                    "                                    var selRecords = Ext.getCmp(\"gridpanelQueryResult\").getSelectionModel().getSelection();\r",
                                    "                                    var str = wordpat;\r",
                                    "                                    var autosendswitch1;\r",
                                    "                                    autosendswitch1=\"关\";\r",
                                    "                                    var str1 = city;\r",
                                    "                                    var str2 = autosendswitch1;\r",
                                    "                                    selRecords[0].set('city',str1);\r",
                                    "                                    selRecords[0].set('wordpat',str);\r",
                                    "                                    selRecords[0].set('autosendswitch',str2);\r",
                                    "                                    var ci = city.split(',');\r",
                                    "                                    selRecords[0].set('number',ci.length);\r",
                                    "                                    selRecords[0].raw.wordpat = wordpat;\r",
                                    "                                    selRecords[0].raw.city = city;\r",
                                    "                                    oldwordpat = wordpat;\r",
                                    "                                    selRecords[0].raw.autosendswitch = \"0\";\r",
                                    "                                    selRecords[0].commit();}else{\r",
                                    "                                }\r",
                                    "                            }else{\r",
                                    "                                var gridpanelQueryResult = Ext.getCmp('gridpanelQueryResult');\r",
                                    "                                var ToolBar = Ext.getCmp(\"ToolBar\");\r",
                                    "                                var page = ToolBar.store.currentPage;\r",
                                    "                                gridpanelQueryResult.store.loadPage(page);\r",
                                    "                                if(retobj.checkInfo==\"插入成功!\"||retobj.checkInfo==\"相似问题插入成功!\"){\r",
                                    "                                    win.close();\r",
                                    "                                }\r",
                                    "                            }\r",
                                    "                        },\r",
                                    "                        //获取数据失败\r",
                                    "                        failure: function (result, request) {\r",
                                    "                            Ext.MessageBox.alert('系统异常', '请求数据失败！');\r",
                                    "                        }\r",
                                    "                    });\r",
                                    "                    //简单词模添加结束\r",
                                    "                    }\r",
                                    "\r",
                                    "\r",
                                    "                }\r",
                                    "            }\r",
                                    "        }\r",
                                    "    ]\r",
                                    "});\r",
                                    "win.show();"
