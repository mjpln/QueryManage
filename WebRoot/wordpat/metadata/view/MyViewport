                                    "    Ext.MessageBox.alert(\"提示消息\", \"请确认词模格式是否和词模类型一致\");\r",
                                    "        return false;\r",
                                    "}\r",
                                    "\r",
                                    "if(wordpattypename ==\"等于词模\"&&wordpat.indexOf(\"类-\")==-1){\r",
                                    "    Ext.MessageBox.alert(\"提示消息\", \"请确认词模格式是否和词模类型一致\");\r",
                                    "        return false;\r",
                                    "}\r",
                                    "\r",
                                    "if(wordpattypename == \"等于词模\"){\r",
                                    "\t\t\t\twordpattype=\"1\";\r",
                                    "\t\t\t}\r",
                                    "\t\t\telse if (wordpattypename == \"排除词模\") {\r",
                                    "\t\t\t\twordpattype=\"2\";\r",
                                    "\t\t\t} else if (wordpattypename ==\"选择词模\") {\r",
                                    "\t\t\t\twordpattype=\"3\";\r",
                                    "\t\t\t} else if (wordpattypename == \"特征词模\") {\r",
                                    "\t\t\t\twordpattype=\"4\";\r",
                                    "            }else{\r",
                                    "                wordpattype=\"0\";\r",
                                    "            }\r",
                                    "\r",
                                    "\r",
                                    "\r",
                                    "\r",
                                    "\r",
                                    "\r",
                                    "\r",
                                    "                          if(wordpattypename==\"选择词模\"){\r",
                                    "                      wordpat = \"++*\"+wordpat;\r",
                                    "                     }else if(wordpattypename==\"排除词模\"){\r",
                                    "                      wordpat = \"~*\"+wordpat;\r",
                                    "                      }else if(wordpattypename==\"特征词模\"){\r",
                                    "                      wordpat = \"+*\"+wordpat;\r",
                                    "                     }\r",
                                    "\r",
                                    "                    var param={};\r",
                                    "                    param[\"cityids\"]=cityids;\r",
                                    "                    param[\"brand\"]=brand;\r",
                                    "                    param[\"service\"]=service;\r",
                                    "                    param[\"topic\"]=topic;\r",
