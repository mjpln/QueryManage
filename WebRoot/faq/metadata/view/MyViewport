                                    "type": "basiceventbinding",
                                    "reference": {
                                        "name": "listeners",
                                        "type": "array"
                                    },
                                    "codeClass": null,
                                    "userConfig": {
                                        "fn": "onFaqgridpanelRender",
                                        "implHandler": [
                                            "//填充客户类型\r",
                                            "Ext.Ajax.request({\r",
                                            "    url: '../faq.action',\r",
                                            "    method: 'post',\r",
                                            "    async: false,\r",
                                            "    params:{type:'customer'},\r",
                                            "    success: function (result, request) {\r",
                                            "        var customertypeitems = Ext.JSON.decode(result.responseText).root;\r",
                                            "        var customertype = Ext.getCmp('customertype');\r",
                                            "        var item ;\r",
                                            "        for(var i=0;i<customertypeitems.length;i++){\r",
                                            "            if(customertypeitems[i]['customertype']=='普通客户'){\r",
                                            "                item =  new Ext.form.Radio({boxLabel:customertypeitems[i]['customertype'],name:'customertype',checked:true, xtype: 'radiofield'});\r",
                                            "            }else{\r",
                                            "                item =  new Ext.form.Radio({boxLabel:customertypeitems[i]['customertype'],name:'customertype', xtype: 'radiofield'});\r",
                                            "            }\r",
                                            "            customertype.items.add(item);\r",
                                            "        }\r",
